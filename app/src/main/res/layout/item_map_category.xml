<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/layout_item_map_category"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/gridview_selector">

        <com.trams.joonggu_nubigo.view.customview.CustomTextViewNomal
            android:id="@+id/tv_name_item_map_category"
            android:layout_width="fill_parent"
            android:layout_height="80dp"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="abcd"
            android:textColor="@android:color/black"
            android:textSize="15sp" />

        <View
            android:id="@+id/v_item_line_view_right"
            android:layout_width="@dimen/fragment_map_category_line_view"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@color/line_view_grid_accessbility_map" />

        <View
            android:id="@+id/v_item_line_view_bottom"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/fragment_map_category_line_view"
            android:layout_alignParentBottom="true"
            android:background="@color/line_view_grid_accessbility_map" />


    </RelativeLayout>
